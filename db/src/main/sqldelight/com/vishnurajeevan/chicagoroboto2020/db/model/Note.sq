CREATE TABLE note (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    description TEXT NOT NULL
);

selectAll:
SELECT * FROM note;

insert:
INSERT INTO note(id, title, description)
VALUES ?;

update:
UPDATE note
SET title = ?, description = ?
WHERE id = ?;

delete:
DELETE FROM note
WHERE id = ?;

count:
SELECT COUNT(*) FROM note;
